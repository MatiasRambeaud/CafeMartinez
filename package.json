{
  "name": "cafe-martinez",
  "version": "1.0.0",
  "description": "Cafe Martinez",
  "main": "src/app.js",
  "type": "module",
  "author": "Matias Rambeaud",
  "license": "ISC",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node src/app.js",
    "server": "nodemon src/app.js",
    "client": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "client:test": "cd client && npm test",
    "client:eject": "cd client && npm run eject",
    "dev": "concurrently \"cross-env ENV=.env.development npm run server\" \"npm run client\"",
    "live": "concurrently \"cross-env ENV=.env.production npm run server\" \"npm run client\""
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "nodemon": "^3.0.0"
  }
}
